package tests{    import flash.geom.Point;    [Unit(label="Comparing different methods of casting a value from an array", loops="100000")]    public class Casting    {        //======================================================================        // Variables        //======================================================================        private var points:Array;        private var i:int;        //======================================================================
        //  Public methods
        //======================================================================        [Before]        public function setUp():void        {            points = [];            for (i = 0; i < 1000000; i++)            {                points[i] = new Point(i, i / 3);            }            i = 0;        }        [Test(label="implicit")]        public function none():void        {            var pt:Point = points[i];            var x:Number = pt.x + 1;            var y:Number = pt.y + 1;            i++;        }        [Test(label="constructor-style")]        public function constructorCasting():void        {            var pt:Point = Point(points[uint(i)]);            var x:Number = pt.x + 1;            var y:Number = pt.y + 1;            i++;        }        [Test(label="operator")]        public function asCasting():void        {            var pt:Point = points[i as uint] as Point;            var x:Number = pt.x + 1;            var y:Number = pt.y + 1;            i++;        }    }}